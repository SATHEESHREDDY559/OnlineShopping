<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
    <servlet-name>GetProductServlet</servlet-name>
    <servlet-class>GetProductServlet</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>GetProductServlet</servlet-name>
    <url-pattern>/getProducts</url-pattern>
  </servlet-mapping>
<servlet>
    <servlet-name>GetCartDetails</servlet-name>
    <servlet-class>GetCartDetails</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>GetCartDetails</servlet-name>
    <url-pattern>/getCart</url-pattern>
  </servlet-mapping>
<servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>LogoutServlet</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>

<servlet>
    <servlet-name>AddProductsServlet</servlet-name>
    <servlet-class>AddProductsServlet</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>AddProductsServlet</servlet-name>
    <url-pattern>/addProducts</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserHomeServlet</servlet-name>
    <servlet-class>UserHomeServlet</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>UserHomeServlet</servlet-name>
    <url-pattern>/UserHomeServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
    </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
    
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
  </welcome-file-list>
</web-app>